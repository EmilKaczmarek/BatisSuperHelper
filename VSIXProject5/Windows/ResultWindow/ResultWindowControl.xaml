<UserControl x:Class="VSIXProject5.ResultWindowControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             Background="{DynamicResource VsBrush.Window}"
             Foreground="{DynamicResource VsBrush.WindowText}"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300"
             Name="MyToolWindow">
    <Grid Margin="0,0,0,153">
        <DataGrid x:Name="dataGrid" Grid.ColumnSpan="2" Margin="0,2,0,-154" ColumnWidth="Auto" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" SelectionMode="Single" IsReadOnly="True" GridLinesVisibility="None" HeadersVisibility="None"/>
    </Grid>
</UserControl>
